<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ReporteMejoresClientes</servlet-name>
        <servlet-class>pe.edu.pucp.CrewmanSystem.servlets.ReporteMejoresClientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportePedidos</servlet-name>
        <servlet-class>pe.edu.pucp.CrewmanSystem.servlets.ReportePedidos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReporteMejoresEmpleados</servlet-name>
        <servlet-class>pe.edu.pucp.CrewmanSystem.servlets.ReporteMejoresEmpleados</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportePedidosXCliente</servlet-name>
        <servlet-class>pe.edu.pucp.CrewmanSystem.servlets.ReportePedidosXCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReporteQuejas</servlet-name>
        <servlet-class>pe.edu.pucp.CrewmanSystem.servlets.ReporteQuejas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReporteMejoresClientes</servlet-name>
        <url-pattern>/ReporteMejoresClientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportePedidos</servlet-name>
        <url-pattern>/ReportePedidos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReporteMejoresEmpleados</servlet-name>
        <url-pattern>/ReporteMejoresEmpleados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportePedidosXCliente</servlet-name>
        <url-pattern>/ReportePedidosXCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReporteQuejas</servlet-name>
        <url-pattern>/ReporteQuejas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
